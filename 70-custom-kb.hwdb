# Install this in /etc/udev/hwdb.d/
keyboard:dmi:bvn*:bvr*:bd*:svnAcer*:pn*
keyboard:dmi:bvn*:bvr*:bd*:svnGateway*:pnA0A1*:pvr*
keyboard:dmi:bvn*:bvr*:bd*:svneMachines:pneMachines*E725:pvr*
 KEYBOARD_KEY_ce=end # BrightnessUp -> End
 KEYBOARD_KEY_ef=home # BrightnessDown -> Home
 KEYBOARD_KEY_ae=pagedown # VolumeUp -> PageUp
 KEYBOARD_KEY_b0=pageup # VolumeDown -> PageDown
 KEYBOARD_KEY_d1=right # PageDown -> Right
 KEYBOARD_KEY_c9=left # PageUp -> Left

